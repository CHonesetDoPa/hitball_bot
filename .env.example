# Telegram Bot Configuration
BOT_TOKEN=your_bot_token_here
# Get your bot token from @BotFather on Telegram

# Optional: Bot username (for better command handling and group restrictions)
BOT_USERNAME=your_bot_username_here

# Optional: Rate limiting configuration (in milliseconds)
HIT_COOLDOWN=3000
COMMAND_COOLDOWN=1000

# Optional: Anti-spam configuration
MAX_VIOLATIONS=5
MUTE_TIME=300
VIOLATION_RESET_TIME=60000

# Optional: Debug mode
DEBUG_MODE=false
